{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Example.Gateway": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api?module=Example.Interface.IMyService&method=Hello",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:4455"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": false,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/weatherforecast",
      "publishAllPorts": true
    }
  }
}